<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ageInvoiceSelectFragment">

    <select id="getAgeInvoiceById" resultType="hmap">
        <include refid="sqlIncludes.ageInvoiceSelectFragment"/>
        WHERE invoice_status_id IN (16, 17) and tenant_id = 63
    </select>

    <update id = "updateAgeInvoice">
        <include refid="sqlIncludes.ageInvoiceUpdateFragment"/>
        and tenant_id = 63
    </update>
</mapper>
